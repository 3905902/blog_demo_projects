<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="customProgressBar">
        <!--已经走过的进度条的颜色-->
        <attr name="progress_bar_reach_color" format="color|reference" />

        <!--还没有走到的进度条的颜色-->
        <attr name="progress_bar_unreach_color" format="color|reference" />

        <!--进度比-->
        <attr name="progress_bar_rate" format="float|reference" />
    </declare-styleable>
</resources>